{
  "name": "@leomotors/config",
  "description": "My personal config for node projects, include ESLint Config",
  "version": "1.1.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.24.0",
  "author": {
    "name": "Nutthapat Pongtanyavichai",
    "url": "https://github.com/leomotors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leomotors/node-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint-config",
    "prettier-config",
    "tsconfig"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "prepack": "rm -rf dist && pnpm build && pnpm lint",
    "lint-strict": "eslint src --max-warnings=0"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.7.4",
    "typescript-eslint": "^8.48.1"
  }
}
